te }))\r\n  }\r\n  function handleAiClickRate () {\r\n    if (aiScore.score < 100) { return }\r\n    setAiScore((prev) => ({ score: prev.score - 100, AiClickRate: prev.AiClickRate + 2 }))\r\n  }\r\n  function randomInt (min, max) { // min and max included\r\n    return Math.floor(Math.random() * (max - min + 1) + min)\r\n  }\r\n  useEffect(() => {\r\n    let ai = setInterval(() => { handleAiScore() }, randomInt(100, 400))\r\n    return function cleanup () {\r\n      clearInterval(ai)\r\n    }\r\n  }, [aiScore])\r\n  useEffect(() => {\r\n    let clickRate = setInterval(() => handleAiClickRate(), 20000)\r\n    return function cleanup () {\r\n      clearInterval(clickRate)\r\n    }\r\n  }, [aiScore.AiClickRate])\r\n  return (\r\n    <>\r\n      <div> opponent:{randomName} </div>\r\n      <div>opponent's score: {aiScore.score} </div>\r\n    </>\r\n  )\r\n}\r\n","C:\\Users\\gfsok\\OneDrive\\Documents\\Visual Studio Code\\React\\Cat Clicker\\my-app\\src\\components\\BackDrop.js",[],"C:\\Users\\gfsok\\OneDrive\\Documents\\Visual Studio Code\\React\\Cat Clicker\\my-app\\src\\images\\emojies.js",[],"C:\\Users\\gfsok\\OneDrive\\Documents\\Visual Studio Code\\React\\Cat Clicker\\my-app\\src\\components\\auto\\AutomateButton.js",[],"C:\\Users\\gfsok\\OneDrive\\Documents\\Visual Studio Code\\React\\Cat Clicker\\my-app\\src\\components\\startMenu\\StartMenu.js",[],{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","severity":1,"message":"77","line":1,"column":27,"nodeType":"78","messageId":"79","endLine":1,"endColumn":36},{"ruleId":"76","severity":1,"message":"80","line":2,"column":8,"nodeType":"78","messageId":"79","endLine":2,"endColumn":16},{"ruleId":"76","severity":1,"message":"81","line":2,"column":20,"nodeType":"78","messageId":"79","endLine":2,"endColumn":55},{"ruleId":"76","severity":1,"message":"82","line":5,"column":8,"nodeType":"78","messageId":"79","endLine":5,"endColumn":19},{"ruleId":"83","severity":1,"message":"84","line":34,"column"