omName = playerName(names)\r\n\r\nexport default function Ai ({ setAiScore, aiScore }) {\r\n  function handleAiScore (prev) {\r\n    setAiScore((prev) => ({ score: (prev.score + prev.AiClickRate), AiClickRate: prev.AiClickRate }))\r\n  }\r\n  function handleAiClickRate () {\r\n    if (aiScore.score < 100) { return }\r\n    setAiScore((prev) => ({ score: prev.score - 100, AiClickRate: prev.AiClickRate + 2 }))\r\n  }\r\n  function randomInt (min, max) { // min and max included\r\n    return Math.floor(Math.random() * (max - min + 1) + min)\r\n  }\r\n  useEffect(() => {\r\n    const ai = setInterval(() => { handleAiScore() }, randomInt(100, 400))\r\n    return function cleanup () {\r\n      clearInterval(ai)\r\n    }\r\n  }, [aiScore])\r\n  useEffect(() => {\r\n    const clickRate = setInterval(() => handleAiClickRate(), 20000)\r\n    return function cleanup () {\r\n      clearInterval(clickRate)\r\n    }\r\n  }, [aiScore.AiClickRate])\r\n  return (\r\n    <div className='scoreContainer opponent'>\r\n      <h4> opponent:<br />{randomName} </h4>\r\n      <h4>{`${aiScore.score} ${aiScore.score != 1 ? 'cats' : 'cat'}`} </h4>\r\n    </div>\r\n  )\r\n}\r\n","C:\\Users\\gfsok\\OneDrive\\Documents\\Visual Studio Code\\React\\Cat Clicker\\my-app\\src\\images\\floatingCats.js",["131","132","133","134","135"],"import anime from './floatingCats/anime.png'\r\nimport catWearCat from './floatingCats/catWearCat.png'\r\nimport chibi from './floatingCats/chibi.png'\r\nimport cuteBrown from './floatingCats/cuteBrown.png'\r\nimport cuteOrange from './floatingCats/cuteOrange.png'\r\nimport loveCats from './floatingCats/loveCats.png'\r\nimport catFace from './floatingCats/cat-face.png'\r\nimport catFace2 from './floatingCats/cat-face2.png'\r\nimport catFace3 from './floatingCats/cat-face3.png'\r\nimport slopy from './floatingCats/slopy.png'\r\nimport stripe from './floatingCats/stripe.png'\r\nimport touchToes from './floatingCats/touchToes.png'\r\nimport naruto from './floatingCats/naruto.